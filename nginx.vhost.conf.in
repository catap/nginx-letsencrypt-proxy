upstream @@DOMAIN@@_backend {
    server @@BACKEND@@;
}

server {
  listen 443 ssl;
  listen [::]:443 ssl;
  
  server_name @@DOMAIN@@;

  ssl_certificate /etc/letsencrypt/live/@@DOMAIN@@/fullchain.pem;
  ssl_certificate_key /etc/letsencrypt/live/@@DOMAIN@@/privkey.pem;  

  location / {
    proxy_pass http://@@DOMAIN@@_backend;
    include proxy_params;
    include @@DOMAIN@@.*.conf;
  }
}